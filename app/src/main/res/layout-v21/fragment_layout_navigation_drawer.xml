<com.velli.passwordmanager.widget.ScrimInsetsFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    app:frameInsetForeground="#4000">


    <ListView
        android:id="@+id/navigation_drawer_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:choiceMode="singleChoice"
        android:divider="@null"
        android:listSelector="@drawable/selectable_background_apptheme"></ListView>

</com.velli.passwordmanager.widget.ScrimInsetsFrameLayout>